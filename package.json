{
  "name": "egg-etcd-discovery",
  "version": "1.3.0",
  "description": "provides egg bindings for the etcd discovery.",
  "eggPlugin": {
    "name": "eggEtcdDiscovery"
  },
  "keywords": [
    "egg",
    "etcd",
    "discovery",
    "egg-etcd-discovery",
    "egg-plugin"
  ],
  "dependencies": {
    "etcd3": "~1.1.2"
  },
  "scripts": {
    "build": "tsc",
    "clean": "tsc -b --clean",
    "ci": "egg-bin pkgfiles --check && npm run lint",
    "lint": "eslint .",
    "pkgfiles": "egg-bin pkgfiles",
    "dev": "cross-env SERVER_IP=DEV_NODE egg-bin dev --port=3544",
    "test": "egg-bin test"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "~1.3.3",
    "egg": "~3.27.1",
    "egg-bin": "~5.18.1",
    "eslint": "^8.57.0",
    "eslint-config-egg": "~12.3.1",
    "egg-mock": "~5.12.5",
    "mocha": "~10.7.0",
    "@types/mocha": "~10.0.7",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "config",
    "lib",
    "*.js",
    "*.ts"
  ],
  "ci": {
    "version": "18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uccu/egg-etcd-discovery.git"
  },
  "bugs": {
    "url": "https://github.com/uccu/egg-etcd-discovery/issues"
  },
  "homepage": "https://github.com/uccu/egg-etcd-discovery#README",
  "author": "cat@4moe.com",
  "license": "MIT"
}