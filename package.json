{
  "name": "egg-etcd-discovery",
  "version": "1.1.10-pre2",
  "description": "provides egg bindings for the etcd discovery.",
  "eggPlugin": {
    "name": "eggEtcdDiscovery"
  },
  "keywords": [
    "egg",
    "etcd",
    "discovery",
    "egg-etcd-discovery",
    "egg-plugin"
  ],
  "dependencies": {
    "etcd3": "^1.1.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "tsc -b --clean",
    "ci": "egg-bin pkgfiles --check && npm run lint",
    "lint": "eslint .",
    "pkgfiles": "egg-bin pkgfiles",
    "dev": "npm run build && egg-bin dev"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "1",
    "egg": "^3",
    "egg-bin": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12",
    "typescript": "^4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "config",
    "lib",
    "*.js",
    "*.ts"
  ],
  "ci": {
    "version": "14, 18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uccu/egg-etcd-discovery.git"
  },
  "bugs": {
    "url": "https://github.com/uccu/egg-etcd-discovery/issues"
  },
  "homepage": "https://github.com/uccu/egg-etcd-discovery#README",
  "author": "cat@4moe.com",
  "license": "MIT"
}